описать ( 'Мои первые тесты' ,  ( )  =>  {

const  page_url = "https://www.tut.by/"

const  search_field = "input [id = 'search_from_str']"

  it ( 'При посещении Google отображается правильный заголовок' ,  ( )  =>  {
    cy . посетить ( page_url )
    cy . получить ( search_field ) . типа ( "Лукашенко" ) ;
    cy . get ( "input [class = 'button big']" ) . click ( ) ;
    cy . получить ( "dl [id = 'областей']" ) . должен ( "быть.видим" ) ;


  } )
} )